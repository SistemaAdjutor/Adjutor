  object QbuscaPedidos: TSQLQuery
    MaxBlobSize = -1
    Params = <>
    SQL.Strings = (
      
        'select p.ped_codigo, p.ped_dtsaida,  p.ped_vltotal_liq, p.ped_si' +
        'tuacao, nf.nf_num_nfe, nf.nf_qtde, cli.cli_cgc, cli.cli_razao, t' +
        'ipo.opv_descricao, rep.rep_nome, cli.cli_cidade, cli.cli_bairro,' +
        ' trp.trp_razao, trp.trp_fone, nf.nf_placave, nf.nf_pesoliq, nf.n' +
        'f_pesobru, pcx.pcx_descri, emp.emp_fantasia, emp.emp_endere, emp' +
        '.emp_bairro, emp.emp_fone, emp.emp_fax, cli.cli_endere, emp.emp_' +
        'uf, cli.cli_uf, emp.emp_cep,cli.cli_cep, trp.trp_cep, trp.trp_ci' +
        'dade, trp.trp_uf, trp.trp_endere, p.SitExp, p.ped_sep'
      'from ped0000 as p'
      'left join nf0001 as nf'
      'on p.ped_codigo = nf.ped_codigo'
      'join cli0000 as cli'
      'on cli.cli_codigo = p.cli_codigo'
      'join opv0000 as tipo'
      'on tipo.opv_codigo = p.opv_codigo'
      'join rep0000 as rep'
      'on rep.rep_codigo = p.rep_codigo'
      'join trp0000 as trp'
      'on trp.trp_codigo = p.trp_codigo'
      'left join pcx0000 as pcx'
      'on pcx.pcx_codigo = p.pcx_codigo'
      'left join emp0000 as emp'
      'on emp.emp_codigo = nf.emp_codigo'
      ''
      ''
      ' '
      'where p.ped_codigo = '#39'-99999'#39)
    SQLConnection = DBConn
    Left = 32
    Top = 280
  end
