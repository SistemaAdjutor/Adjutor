  object qOperacao: TFDQuery
    Connection = DBInicio.FDACConn
    SQL.Strings = (
      
        'SELECT f.SEQUENCIA, o.OPE_CODIGO, o.OPE_DESCRICAO, f.FAB_CODIGO,' +
        ' eq.EQP_CODIGO, eq.EQP_DESCRICAO'
      '  FROM FABRICACAO f '
      '  JOIN OPERACOES o ON (o.OPE_CODIGO = f.OPE_CODIGO)'
      '  LEFT JOIN EQUIPAMENTO eq ON (eq.EQP_CODIGO = f.EQP_CODIGO)'
      ' WHERE f.OSV_CODIGO = '#39'4'#39
      ' ORDER BY sequencia')
