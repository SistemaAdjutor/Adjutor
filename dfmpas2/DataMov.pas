       end;}
    if Assigned(FrmPedido) then
       FrmPedido.BuscaItensNota;

end;

procedure TDataMovimento.buscaItemNota;
begin
    try
      if CdsNotaFiscal.Active then
        begin
         CdsItemNota.Close;
         SqlItemNota.sql.text := 'Select * from NF_IT01 where NF_IT_NOTANUMER = '''+CdsNotaFiscalNF_NOTANUMBER.AsString+'''';
         CdsItemNota.Open;
         CdsItemNota.RecordCount;
       end;
    except on E:EdatabaseError do
      uteis.erro  (pchar('Erro ao localizar itens !'+e.Message))
    end;
end;

