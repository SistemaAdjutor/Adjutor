procedure TDataMovimento.CdsCaixaFixoAfterScroll(DataSet: TDataSet);
begin
    if Assigned(FormCaixaFixo) then
       begin
           if FormCaixaFixo.SqlCdsCtaAnalise.Locate('CCT_CODIGO',FormCaixaFixo.DBEditCtaAnalise.Text,[]) = true then
              begin
                  if FormCaixaFixo.SqlCdsCtaAnaliseCCT_CONTA.AsString = 'S' then
                     // conta tipo despesa = Sada
                     FormCaixaFixo.CCxa_valor.Value := DataMovimento.CdsCaixaFixoCXA_SAIDA.AsCurrency
                  else
                     // conta tipo Receita = Enttrada
                     FormCaixaFixo.CCxa_valor.Value := DataMovimento.CdsCaixaFixoCXA_ENTRADA.AsCurrency;
                  FormCaixaFixo.CbCtaAnalise.Text   := FormCaixaFixo.SqlCdsCtaAnaliseCCT_DESCRI.AsString;
                  FormCaixaFixo.CbTipoDoc.ItemIndex := IIF(DataMovimento.CdsCaixaFixoCXA_TIPODOC.AsString = 'D',0,IIF(DataMovimento.CdsCaixaFixoCXA_TIPODOC.AsString = 'C',1,IIF(DataMovimento.CdsCaixaFixoCXA_TIPODOC.AsString = 'O',2,3)));
              end
           else
              begin
                  FormCaixaFixo.CbCtaAnalise.Text   := '';
                  if DataMovimento.CdsCaixaFixoCXA_TIPODOC.AsString = 'T' then
                     FormCaixaFixo.CCxa_valor.Value := DataMovimento.CdsCaixaFixoCXA_ENTRADA.AsCurrency;
              end;
       end;
end;

procedure TDataMovimento.CdsFechaCaixaCalcFields(DataSet: TDataSet);
begin
    CdsFechaCaixaCC_SALDOCAIXA.AsCurrency :=(CdsFechaCaixaCXF_ENTRADAS.AsCurrency-CdsFechaCaixaCXF_SAIDAS.AsCurrency)+CdsFechaCaixaCXF_ANTERIOR.AsCurrency;
end;

procedure TDataMovimento.CdsItemNotaCalcFields(DataSet: TDataSet);
begin
    CdsItemNotaNF_TOTAL_CC.AsCurrency     := (CdsItemNotaNF_QTDE.AsCurrency * CdsItemNotaNF_PRECO.AsCurrency);
    CdsItemNotaNF_TOTAL_IPI_CC.AsCurrency := (((CdsItemNotaNF_QTDE.AsCurrency * CdsItemNotaNF_PRECO.AsCurrency) * CdsItemNotaNF_IPIALIQ.AsCurrency)/100)+(CdsItemNotaNF_QTDE.AsCurrency * CdsItemNotaNF_PRECO.AsCurrency);
end;

