begin
    CdsPedItemPRF_TOTAL_CC.AsCurrency       := (CdsPedItemPRF_QTDE.AsCurrency * CdsPedItemPRF_PRECO.AsCurrency);
    CdsPedItemPRF_TOTAL_BRUTO_CC.AsCurrency := (CdsPedItemPRF_QTDE.AsCurrency * CdsPedItemPRF_PRECO_BRUTO.AsCurrency);
