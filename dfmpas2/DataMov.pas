procedure TDataMovimento.BuscaItemEmb;
begin
   {busca item ref a embalagem}
 try
   if CdsEmbalagem.Active then
      begin
         if CdsEmbalagem.IsEmpty = false then
            begin
               CdsItemEmb.Close;
               SqlItemEmb.sql.text := SQLDEF('PEDIDOS','select PED.* ,PRD_DESCRI from PED_EB03 PED left join PRD0000 on PRD0000.PRD_REFER = PED_EB03.PRD_REFER','where PEB_REGISTRO = '''+CdsEmbalagemPEB_REGISTRO.AsString+'''','PED.PEB_REGISTRO','PED.');
               CdsItemEmb.Open;
            end;
      end;
 except on E:EdatabaseError do
    uteis.erro  (Pchar('Erro ao localizar itens ref a embalagem !'+e.message));
 end;
end;

procedure TDataMovimento.CdsPedItemPRF_QTDE_FATURAR_CCChange(
  Sender: TField);
begin
 if Assigned(FormFaturamento) then
   begin
     If (DataMovimento.CdsPedItemPRF_QTDE.Ascurrency - DataMovimento.CdsPedItemPRF_QTDEFAT.AsCurrency ) < CdsPedItemPRF_QTDE_FATURAR_CC.AsCurrency then
        begin
            uteis.aviso('Quantidade selecionada maior que a quantidade pendente !');
            CdsPedItemPRF_QTDE_FATURAR_CC.AsCurrency := DataMovimento.CdsPedItemPRF_QTDE.Ascurrency - DataMovimento.CdsPedItemPRF_QTDEFAT.AsCurrency;
        end;
    end;
end;

