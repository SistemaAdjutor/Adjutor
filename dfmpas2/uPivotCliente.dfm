  object cdsBusca: TFDQuery
    Connection = dbConn
    SQL.Strings = (
      ' SELECT it.NF_QTDE , COALESCE(it.NF_PRECO,0) as NF_PRECO ,'
      '(it.NF_QTDE * COALESCE(it.NF_PRECO,0)) AS NF_TOT_NOTA,'
      ' nf.cli_codigo, cli_razao, nf.NF_EMISSAO, pr.PRD_REFER, '
      ' tp.PTI_CODIGO, tp.PTI_DESCRI,'
      ' pr.PRD_DESCRI, li.LIN_CODIGO, li.LIN_DESCRI, '
      ' gr.PGR_CODIGO, gr.PGR_DESCRI, ga.PRG_DESCRICAO,'
      ' nf.EMP_CODIGO,  nf.EMP_CODIGO||'#39'-'#39'||em.EMP_RAZAO AS empresa,'
      ' RE.REP_CODIGO, RE.REP_NOME, it.NTP_CFOP, '
      ' it.NF_IPIVALOR, it.NF_CUSTO, it.NF_VLSUBST, it.NF_ICMSVALOR,'
      'it.NF_COMISSAO, it.NF_VLPIS, it.NF_VLCOFINS, '
      
        'pc.PCL_CODIGO, pc.PCL_NOME, px.PCX_CODIGO, px.PCX_DESCRI, fp.FPG' +
        '_REGISTRO , fp.FPG_DESCRICAO,'
      'FT.CCT_CODIGO, (ct.CCT_NIVEL||'#39'-'#39'|| CT.CCT_DESCRI) as CCT_DESCRI'
      ' FROM NF_IT01 it'
      
        ' JOIN NF0001 nf ON (nf.NF_NOTANUMBER = it.NF_IT_NOTANUMER AND nf' +
        '.EMP_CODIGO = it.EMP_CODIGO) '
      ' JOIN EMP0000 em ON (nf.EMP_CODIGO = em.EMP_CODIGO) '
      ' JOIN CLI0000 cl ON (cl.CLI_CODIGO = nf.cli_codigo)'
      ' JOIN PRD0000 pr ON (pr.PRD_codigo = it.PRD_codigo)'
      ' JOIN PRD_TIPO tp ON (tp.PTI_CODIGO = pr.PTI_CODIGO)'
      ' LEFT JOIN PRD_LINHA li ON (li.LIN_CODIGO = pr.LIN_CODIGO) '
      ' LEFT JOIN PRD_GRUPO gr ON (gr.PGR_CODIGO = pr.PGR_CODIGO)'
      ' LEFT JOIN REP0000 RE on (RE.REP_CODIGO = NF.REP_CODIGO)'
      ' LEFT JOIN PRD_GRADE ga ON (ga.prd_codigo = pr.PRD_CODIGO)'
      ' LEFT JOIN PCL0000 pc ON (nf.PCL_CODIGO = pc.PCL_CODIGO)'
      
        ' LEFT JOIN FAT0000 FT ON (FT.fat_codigo = NF.NF_NOTANUMBER  AND ' +
        ' ft.EMP_CODIGO = nf.emp_codigo)  '
      ' LEFT JOIN PCX0000 px ON (px.PCX_CODIGO = ft.PCX_CODIGO)'
      
        ' LEFT JOIN FORMA_PAGAMENTO FP on (FP.FPG_REGISTRO = NF.FPG_REGIS' +
        'TRO ) '
      ' LEFT JOIN CCT_0000 CT ON CT.CCT_CODIGO = FT.CCT_CODIGO'
      ' WHERE nf.NF_VENDA_FATURADA = '#39'S'#39
      ' AND nf.NF_CANCELADA <> '#39'S'#39' ')
    Left = 363
    Top = 208
